<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Map_Pic">

    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="animal"
            android:ems="10"
            android:id="@+id/animName"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="424dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="48dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="imageBase"
            android:ems="10"
            android:id="@+id/imageBase64"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="60dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="lng"
            android:ems="10"
            android:id="@+id/mapLng"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="156dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="ltd"
            android:ems="10"
            android:id="@+id/mapLtd"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="252dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="user"
            android:ems="10"
            android:id="@+id/userID"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="340dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="48dp"/>
    <Button
            android:text="MapAnimal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mapBt"
            app:layout_constraintTop_toBottomOf="@+id/animName" android:layout_marginTop="80dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="44dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>